<createPlan>
   <apiUrl>%s(apiUrl)</apiUrl>
   <apiKey>%s(apiKey)</apiKey>
   <planCode>%s(planCodeOptional)</planCode>
   <name>%s(nameString)</name>
   <setupFeeInCents>
      <values>
         <USD>1000</USD>
         <EUR>800</EUR>
      </values>
   </setupFeeInCents>
   <unitAmountInCents>
      <values>
         <USD>6000</USD>
         <EUR>4500</EUR>
      </values>
   </unitAmountInCents>
   <planIntervalLength>1</planIntervalLength>
   <planIntervalUnit>months</planIntervalUnit>
   <taxExempt>false</taxExempt>
   <accountingCode>accounting</accountingCode>
   <trialIntervalUnit>months</trialIntervalUnit>
   <trialIntervalLength>2</trialIntervalLength>
   <totalBillingCycles>5</totalBillingCycles>
   <unitName>UnitName</unitName>
   <displayQuantity>true</displayQuantity>
   <successUrl>http://www.virtusax.com</successUrl>
   <cancelUrl>http://www.virtusax.com</cancelUrl>
   <taxCode>unknown</taxCode>
</createPlan>